[epel]
name=Extra Packages for Enterprise Linux {{epel_version}} - $basearch
#baseurl=http://download.fedoraproject.org/pub/epel/{{epel_version}}/$basearch
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-{{epel_version}}&arch=$basearch
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{epel_version}}

[epel-debuginfo]
name=Extra Packages for Enterprise Linux {{epel_version}} - $basearch - Debug
#baseurl=http://download.fedoraproject.org/pub/epel/{{epel_version}}/$basearch/debug
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-debug-{{epel_version}}&arch=$basearch
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{epel_version}}
gpgcheck=1

[epel-source]
name=Extra Packages for Enterprise Linux {{epel_version}} - $basearch - Source
#baseurl=http://download.fedoraproject.org/pub/epel/{{epel_version}}/SRPMS
mirrorlist=https://mirrors.fedoraproject.org/metalink?repo=epel-source-{{epel_version}}&arch=$basearch
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-{{epel_version}}
gpgcheck=1
